#include <stdio.h>

int square(int num);
int main() {
    int i, j;

    for(i=1; i<=1000; i++) {
        for(j=1; j<=1000; j++) {
            if (square(i)+square(j)== square(1000-i-j))
            break;


        }
    if (square(i)+square(j)== square(1000-i-j))
    break;


    }

printf("%d %d %d", i, j, 1000-i-j);

    

return 0;
}
int square(int num)
{
    int result=num*num;
    return result;

}
